{
  "name": "goof",
  "version": "1.0.1",
  "description": "A vulnerable todo demo application",
  "homepage": "https://snyk.io/",
  "repository": {
    "type": "git",
    "url": "https://github.com/Snyk/snyk-todo-list-demo-app/"
  },
  "scripts": {
    "dev": "NODE_OPTIONS=--openssl-legacy-provider nodemon ./app.js",
    "start": "NODE_OPTIONS=--openssl-legacy-provider node app.js",
    "build": "browserify -r jquery > public/js/bundle.js",
    "cleanup": "mongo express-todo --eval 'db.todos.remove({});'",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --reporters=default --reporters=jest-junit --coverage=false",
    "lint": "eslint .",
    "security-scan": "npm audit && snyk test"
  },
  "jest-junit": {
    "outputDirectory": "coverage",
    "outputName": "junit.xml"
  },
  "dependencies": {
    "adm-zip": "^0.4.16",
    "body-parser": "^1.20.3",
    "cfenv": "^1.0.4",
    "consolidate": "0.14.5",
    "dustjs-helpers": "^1.1.2",
    "dustjs-linkedin": "^3.0.1",
    "ejs": "1.0.0",
    "ejs-locals": "1.0.2",
    "errorhandler": "^1.5.1",
    "express": "^4.21.2",
    "express-fileupload": "^1.5.1",
    "express-session": "^1.17.2",
    "file-type": "^8.1.0",
    "hbs": "^4.0.4",
    "humanize-ms": "^1.2.1",
    "jquery": "^3.7.1",
    "junit": "^0.5.1",
    "lodash": "^4.17.21",
    "marked": "^0.3.19",
    "method-override": "latest",
    "moment": "^2.30.1",
    "mongodb": "^3.5.9",
    "mongoose": "^8.15.1",
    "morgan": "latest",
    "ms": "^2.1.3",
    "mysql": "^2.18.1",
    "npmconf": "^2.1.3",
    "optional": "^0.1.3",
    "st": "^3.0.1",
    "stream-buffers": "^3.0.1",
    "tap": "^21.1.0",
    "typeorm": "^0.3.24",
    "validator": "^13.5.2"
  },
  "devDependencies": {
    "@jest/reporters": "^29.7.0",
    "browserify": "^13.1.1",
    "istanbul-reports": "^3.1.7",
    "jest": "^29.7.0",
    "jest-junit": "^16.0.0",
    "nodemon": "^2.0.7",
    "snyk": "^1.244.0"
  },
  "license": "Apache-2.0"
}
